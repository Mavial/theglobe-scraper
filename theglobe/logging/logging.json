{
        "version": 1,
        "disable_existing_loggers": false,
        "loggers": {
            "": {
                "level": "NOTSET",
                "handlers": ["console_handler", "error_file_handler"]
        },
            "__main__": {
                "level": "INFO",
                "handlers": ["main_rotating_file_handler", "error_file_handler"
            ]
        },
            "theglobe.database.insert": {
                "level": "INFO",
                "handlers": ["theglobe_database_insert_rotating_file_handler","error_file_handler"
            ]
        },
            "theglobe.logging.init_logging": {
                "level": "INFO",
                "handlers": ["theglobe_logging_init_logging_rotating_file_handler","error_file_handler"
            ]
        },
            "theglobe.scraper.scrape": {
                "level": "INFO",
                "handlers": ["theglobe_scraper_scrape_rotating_file_handler","error_file_handler"
            ]
        }
    },
        "handlers": {
            "console_handler": {
                "level": "INFO",
                "formatter": "default_console",
                "class": "logging.StreamHandler",
                "stream": "ext://sys.stdout"
        },
            "main_rotating_file_handler": {
                "level": "DEBUG",
                "formatter": "default_file",
                "class": "logging.handlers.RotatingFileHandler",
                "filename": "./theglobe/logging/tmp/main.log",
                "mode": "a",
                "maxBytes": 1048576,
                "backupCount": 10
        },
            "theglobe_database_insert_rotating_file_handler": {
                "level": "DEBUG",
                "formatter": "default_file",
                "class": "logging.handlers.RotatingFileHandler",
                "filename": "./theglobe/logging/tmp/theglobe_database_insert.log",
                "mode": "a",
                "maxBytes": 1048576,
                "backupCount": 10
        },
            "theglobe_logging_init_logging_rotating_file_handler": {
                "level": "DEBUG",
                "formatter": "default_file",
                "class": "logging.handlers.RotatingFileHandler",
                "filename": "./theglobe/logging/tmp/theglobe_logging_init_logging.log",
                "mode": "a",
                "maxBytes": 1048576,
                "backupCount": 10
        },
            "theglobe_scraper_scrape_rotating_file_handler": {
                "level": "DEBUG",
                "formatter": "default_file",
                "class": "logging.handlers.RotatingFileHandler",
                "filename": "./theglobe/logging/tmp/theglobe_scraper_scrape.log",
                "mode": "a",
                "maxBytes": 1048576,
                "backupCount": 10
        },
            "error_file_handler": {
                "level": "ERROR",
                "formatter": "default_file",
                "class": "logging.FileHandler",
                "filename": "./theglobe/logging/tmp/error.log",
                "mode": "a"
        }
    },
        "formatters": {
        "default_console": {
            "()": "colorlog.ColoredFormatter",
            "format": "%(blue)s[%(asctime)s] %(log_color)s%(levelname)-8s [%(name)s] [%(module)s:%(lineno)s] %(white)s: %(message)s"
        },
        "default_file": {
            "format": "[%(asctime)s] %(levelname)-8s [%(name)s] [%(module)s:%(lineno)s] : %(message)s"
        }
    }
}